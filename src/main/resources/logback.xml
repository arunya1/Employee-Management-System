<?xml version="1.0" encoding="UTF-8"?>

<configuration>



 <appender name="DEBUG" class="ch.qos.logback.core.ConsoleAppender">
   <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %m %n</pattern>
    </encoder>
</appender>

 <root level="debug">
   <appender-ref ref="DEBUG" />
 </root> 
 
